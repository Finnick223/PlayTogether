{
  "printWidth": 80,
  "tabWidth": 3,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxSingleQuote": true,
  "quoteProps": "as-needed",
  "arrowParens": "always",
  "insertPragma": false,
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-tailwindcss"]
}
